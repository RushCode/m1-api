{
  "name": "leocata/m1-api",
  "description": "PHP API M1",
  "keywords": ["M1","php","curl","api","client", "sdk","rest","restful"],
  "license": "MIT",
  "type": "library",
  "minimum-stability": "alpha",
  "version": "0.0.2",
  "authors": [
    {
      "name": "Leo Cata",
      "email": "leocata@yandex.ru"
    }
  ],
  "support": {
    "issues": "https://github.com/RushCode/m1-api/issues",
    "source": "https://github.com/RushCode/m1-api"
  },
  "require": {
    "php": "^7.0",
    "ext-pdo": "*",
    "ext-curl": "*",
    "ratchet/pawl": "0.2.*",
    "react/http-client": "0.5.*",
    "psr/log": "1.0.*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0",
    "phpunit/phpunit": "6.2",
    "phpunit/php-code-coverage": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "leocata\\M1\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "leocata\\M1\\tests\\": "tests"
    }
  }
}